!!!
html
  head: block head
    title loadr
    link(rel="stylesheet", href="http://yandex.st/highlightjs/7.3/styles/default.min.css")
    link(rel="stylesheet", href="/main.css")
    :loadr
      http://yandex.st/highlightjs/7.3/highlight.min.js
    script
      hljs.initHighlightingOnLoad()


  body
    .side-bar: block side-bar
      h1.logo: block logo
        .title: block title
        .from: block logo-from
          | of
        a(href="/") loadr
        .version= settings.version

      ul.links: block side-bar-links
        li: a(href="/docs") documentation
        li: a(href="https://github.com/Wizek/loadr") github repository
        li: a(href="/changelog") changelog
        br
        li: a(href="/cache") cache status


    .content: .content-fixwidth
      block content

    script
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga')

      ga('create', 'UA-41065325-1', 'af.cm')
      ga('send', 'pageview')
